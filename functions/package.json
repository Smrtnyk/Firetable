        "web-push": "3.4.4"
    },
    "devDependencies": {
        "@types/faker": "5.5.3",
        "@types/web-push": "3.3.0",
        "@typescript-eslint/eslint-plugin": "4.22.0",
        "@typescript-eslint/parser": "4.22.0",
        "eslint": "7.25.0",
        "eslint-plugin-import": "2.22.1",
        "faker": "5.5.3",
        "typescript": "4.2.4"
    },
    "private": true
