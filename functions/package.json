        "firebase-admin": "9.11.0",
        "firebase-functions": "3.14.1",
        "web-push": "3.4.5"
    },
    "devDependencies": {
        "@types/faker": "5.5.7",
        "@types/web-push": "3.3.2",
        "@typescript-eslint/eslint-plugin": "4.29.0",
        "@typescript-eslint/parser": "4.29.0",
        "eslint": "7.32.0",
        "eslint-plugin-import": "2.23.4",
        "faker": "5.5.3",
        "typescript": "4.3.5"
    },
    "private": true
