{
    "name": "firetable",
    "workspaces": [
        "packages/functions",
        "packages/frontend",
        "packages/backend",
        "packages/floor-creator",
        "packages/types",
        "packages/utils"
    ],
    "scripts": {
        "deploy:hosting": "firebase deploy --only hosting",
        "deploy:functions": "firebase deploy --only functions",
        "deploy": "firebase deploy",
        "start:emulators": "firebase emulators:start --import=./local-db --export-on-exit=./local-db",
        "seed:events": "node packages/functions/lib/src/seed/add-events.js",
        "seed:generate-admin-local": "node packages/functions/lib/src/seed/generate-admin-local.js",
        "seed:admin-prod": "node packages/functions/lib/src/seed/add-admin-prod.js",
        "dev:frontend": "cd packages/frontend && npm run dev",
        "build:frontend": "cd packages/frontend && quasar build",
        "build:functions": "cd packages/functions && npm run build"
    },
    "devDependencies": {
        "@quasar/app-vite": "1.0.6",
        "autoprefixer": "10.4.8",
        "@types/web-push": "3.3.2",
        "@typescript-eslint/eslint-plugin": "5.32.0",
        "@typescript-eslint/parser": "5.32.0",
        "dotenv": "16.0.1",
        "eslint": "8.21.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-promise": "6.0.0",
        "eslint-plugin-quasar": "1.1.0",
        "eslint-plugin-vue": "9.3.0",
        "prettier": "2.7.1",
        "typescript": "4.7.4",
        "@faker-js/faker": "^7.3.0"
    },
    "dependencies": {
        "@quasar/extras": "1.15.1",
        "@types/fabric": "4.5.11",
        "chart.js": "3.9.1",
        "diff-arrays-of-objects": "1.1.9",
        "fabric": "5.2.1",
        "firebase": "9.9.2",
        "firebase-admin": "11.0.1",
        "firebase-functions": "3.22.0",
        "pinia": "2.0.17",
        "quasar": "2.7.7",
        "vue": "3.2.37",
        "vue-i18n": "9.2.2",
        "vue-router": "4.1.3",
        "web-push": "3.5.0"
    },
    "private": true
}
