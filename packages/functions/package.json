{
    "name": "@firetable/functions",
    "version": "0.0.1",
    "main": "lib/functions/src/index.js",
    "type": "module",
    "scripts": {
        "build": "pnpm run typecheck && tsc -p tsconfig.prod.json",
        "typecheck": "tsc --noEmit",
        "test:local": "vitest",
        "test": "bash run-tests.sh",
        "dev": "tsc --watch",
        "deploy": "firebase deploy --only functions",
        "logs": "firebase functions:log",
        "find-dead-code": "npx ts-prune",
        "seed": "npx tsx seeds/scripts/seed.ts",
        "seed:admin": "npx tsx seeds/scripts/seed.ts --with-admin"
    },
    "dependencies": {
        "@google-cloud/functions-framework": "3.4.5",
        "es-toolkit": "1.31.0",
        "firebase-admin": "13.0.2",
        "firebase-functions": "6.2.0"
    },
    "devDependencies": {
        "@faker-js/faker": "9.4.0",
        "@firebase/app-types": "0.9.3",
        "@firebase/rules-unit-testing": "4.0.1",
        "@types/node": "22.10.10",
        "cac": "6.7.14",
        "tinyrainbow": "2.0.0",
        "vitest": "https://pkg.pr.new/vitest@9e40437"
    },
    "private": true
}
