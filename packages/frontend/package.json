        "@types/papaparse": "5.3.16",
        "@types/qrcode": "1.5.5",
        "@vitejs/plugin-vue": "5.2.4",
        "@vitest/browser": "catalog:",
        "@vitest/coverage-v8": "catalog:",
        "@vitest/ui": "catalog:",
        "@vue/test-utils": "2.4.6",
        "cli-table3": "0.6.5",
        "knip": "catalog:",
        "tinyrainbow": "2.0.0",
        "tsx": "4.19.4",
        "vite-tsconfig-paths": "5.1.4",
        "vitest": "catalog:",
        "vitest-browser-vue": "0.2.0",
