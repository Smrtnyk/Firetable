{
    "name": "@firetable/frontend",
    "version": "0.2.0",
    "description": "Firetable reservation management system",
    "productName": "Firetable",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "quasar dev",
        "dev:pwa": "quasar dev -m pwa",
        "build:spa": "quasar build",
        "build:pwa": "quasar build -m pwa",
        "build": "quasar build -m pwa && pnpm run typecheck",
        "typecheck": "vue-tsc --noEmit",
        "test": "vitest",
        "test:coverage": "vitest run --coverage",
        "find-dead-code": "knip -c ./config/knip.json",
        "check-translations": "tsx ./tools/check-translations.ts"
    },
